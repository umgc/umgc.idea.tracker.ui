{
    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "containerName": {
            "value": "cappms"
        },
        "location": {
            "value": "eastus"
        },
        "imageType": {
            "value": "Public"
        },
        "imageName": {
            "value": "docker.io/umgccaps/project-tracker-ui:latest"
        },
        "osType": {
            "value": "Linux"
        },
        "numberCpuCores": {
            "value": "2"
        },
        "memory": {
            "value": "6"
        },
        "restartPolicy": {
            "value": "OnFailure"
        },
        "environmentVariables": {
            "value": [
                {
                    "name": "Test",
                    "value": "test"
                }
            ]
        },
        "ipAddressType": {
            "value": "Public"
        },
        "ports": {
            "value": [
                {
                    "port": "80",
                    "protocol": "TCP"
                },
                {
                    "port": "4200",
                    "protocol": "TCP"
                }
            ]
        },
        "dnsNameLabel": {
            "value": "project-tracker"
        }
    }
}